---
layout: null
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:https://akstuhl.net
BEGIN:VTIMEZONE
TZID:America/Toronto
X-LIC-LOCATION:America/Toronto
BEGIN:DAYLIGHT
TZOFFSETFROM:-0500
TZOFFSETTO:-0400
TZNAME:EDT
DTSTART:19700308T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=2SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:-0400
TZOFFSETTO:-0500
TZNAME:EST
DTSTART:19701101T020000
RRULE:FREQ=YEARLY;BYMONTH=11;BYDAY=1SU
END:STANDARD
END:VTIMEZONE
METHOD:PUBLISH{% for show in site.tuning %}
BEGIN:VEVENT
UID:{{ show.slug }}@akstuhl.net
ORGANIZER;CN="Andy's radio tuner":MAILTO:andykstuhl@gmail.com
LOCATION:{{ site.data.stations[show.station] }}
SUMMARY:{{ show.name | remove: ',' | remove: ';' }}
DESCRIPTION:{{ show.content | strip_html }} {{ show.more }}
CLASS:PUBLIC
RRULE:FREQ={{ show.frequency | upcase }};BYDAY={{ show.day | truncate: 2, "" | upcase }}
DTSTART;TZID=America/Toronto:20221001T{{ show.start | date: "%H%M%S" }}
DTEND;TZID=America/Toronto:20221001T{{ show.end | date: "%H%M%S" }}
CREATED:{{ show.added | date: "%Y%m%d" }}T170000Z
END:VEVENT{% endfor %}
END:VCALENDAR
